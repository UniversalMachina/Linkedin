{
    "manifest_version": 2,
    "name": "Button Clicker 2.0",
    "description": "This extension will find and click all '1-Click Apply' buttons on a webpage",
    "version": "1.0",
    "permissions": ["activeTab", "tabs", "<all_urls>",  "notifications"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Click Apply Buttons"
    }
  }
  